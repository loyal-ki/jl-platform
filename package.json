        "eslint-import-resolver-babel-module": "^5.3.2",
        "eslint-plugin-import": "^2.25.2",
        "eslint-plugin-jest": "^27.2.1",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-module-resolver": "^1.5.0",
        "eslint-plugin-n": "^15.0.0",
